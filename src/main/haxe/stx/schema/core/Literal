package stx.schema.core;

enum LiteralSum{
  LVal(set:XSet<Option<Knuckle>,>);
}
@:using(stx.schema.core.Literal.LiteralLift)
abstract Literal(LiteralSum) from LiteralSum to LiteralSum{
  static public var _(default,never) = LiteralLift;
  public inline function new(self:LiteralSum) this = self;
  @:noUsing static inline public function lift(self:LiteralSum):Literal return new Literal(self);

  public function prj():LiteralSum return this;
  private var self(get,never):Literal;
  private function get_self():Literal return lift(this);
}
class LiteralLift{
  static public inline function lift(self:LiteralSum):Literal{
    return Literal.lift(self);
  }
}